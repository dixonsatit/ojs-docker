<locale name="sr_SR" full_name="Cрпски">
	<message key="plugins.pubIds.urn.displayName">URN</message>

	<!-- settings form -->
	<message key="plugins.pubIds.urn.manager.settings.urnObjects">Sadržaj časopisa</message>
	<message key="plugins.pubIds.urn.manager.settings.urnObjectsRequired">Odaberi objekte za koje bi URN-ovi trebali biti dodeljeni.</message>
	<message key="plugins.pubIds.urn.manager.settings.enableIssueURN">URN-ovi će biti dodeljeni jedinicama.</message>
	<message key="plugins.pubIds.urn.manager.settings.enableSubmissionURN">URN-ovi će biti dodeljeni člancima.</message>
	<message key="plugins.pubIds.urn.manager.settings.enableRepresentationURN">URN-ovi će biti dodeljeni prikazima (npr. HTML ili PDF faljlovi).</message>

	<message key="plugins.pubIds.urn.manager.settings.urnPrefix">URN prefiks</message>
	<message key="plugins.pubIds.urn.manager.settings.urnPrefix.description">URN prefiks je fiksan, nepromenjiv deo URN-a (npr. "urn:nbn:de:0000-").</message>
	<message key="plugins.pubIds.urn.manager.settings.form.urnPrefixPattern"><![CDATA[Obrazac URN prefiksa mora biti u formi "urn:"&lt;NID&gt;":"&lt;NSS&gt;.]]></message>

	<message key="plugins.pubIds.urn.manager.settings.urnSuffix">URN sufiks</message>
	<message key="plugins.pubIds.urn.manager.settings.urnSuffixPattern.example">Npr, vol%viss%ipp%p može generisati URN sufiks "vol3iss2pp230".</message>
	<message key="plugins.pubIds.urn.manager.settings.urnSuffixPattern.issues">za jedinicu (broj)</message>
	<message key="plugins.pubIds.urn.manager.settings.urnSuffixPattern.submissions">za članak</message>
	<message key="plugins.pubIds.urn.manager.settings.urnSuffixPattern.representations">za prikaz</message>
	<message key="plugins.pubIds.urn.manager.settings.urnIssueSuffixPatternRequired">Unesite URN sufiks obrazac za jedinicu.</message>
	<message key="plugins.pubIds.urn.manager.settings.urnSubmissionSuffixPatternRequired">Unesite URN sufiks obrazac za članak.</message>
	<message key="plugins.pubIds.urn.manager.settings.urnRepresentationSuffixPatternRequired">Unesite URN sufiks obrazac za prikaz.</message>
	<message key="plugins.pubIds.urn.manager.settings.urnSuffixDefault">Koristi predefinisane obrasce.</message>
	<message key="plugins.pubIds.urn.manager.settings.checkNo">Broj za proveru</message>
	<message key="plugins.pubIds.urn.manager.settings.checkNo.label">Broj za proveru biće automatski izračunat i dodat na kraj, kao poslednja cifra URN-a.</message>
	<message key="plugins.pubIds.urn.manager.settings.namespace">Ime</message>
	<message key="plugins.pubIds.urn.manager.settings.namespace.choose">Izaberi</message>
	<message key="plugins.pubIds.urn.manager.settings.namespace.description">Imena stalnih identifikatora obično potrebni za registraciju (e.g. at the Deutsche Nationalbibliothek).</message>

	<message key="plugins.pubIds.urn.manager.settings.urnReassign">Obriši URN-ove</message>
	<message key="plugins.pubIds.urn.manager.settings.urnReassign.description">Ako promenite URN konfiguraciju, URN-ovi koje ste već dodelili neće biti pogođeni promenom. Kada je promenjena URN konfiguracija sačuvana, koristite ovo dugme za brisanje svih postojećih URN-ova, tako da nova podešavanja stupe na snagu sa postojećim objektima.</message>
	<message key="plugins.pubIds.urn.manager.settings.urnReassign.confirm">Da li ste sigurni da želite da obrišete postojeće URN-ove?</message>

	<!-- metadata form -->
	<message key="plugins.pubIds.urn.editor.urn">URN</message>
	<message key="plugins.pubIds.urn.form.customIdentifierNotUnique">Dati URN sufiks je već u upotrebi. Unesite jedinstveni URN sufiks za svaku stavku.</message>
	<!-- errors -->
	<message key="plugins.pubIds.urn.form.checkNoRequired">Broj za proveru nedostaje.</message>

